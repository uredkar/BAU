   "source": [
    "import pandas as pd\n",
    "\n",
    "data = {\n",
    "    \"category\": [\"Italian\", \"Mexican\", \"Italian\", \"Indian\", \"Mexican\", \"Indian\"],\n",
    "    \"rating\": [4.2, 3.8, 4.0, 2.0, 4.5, None]\n",
    "}\n",
    "\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "df['num_rating'] = df.groupby('category')['rating'].apply(lambda x: x.fillna(x.mean()))\n",
    "print(df)"
   ]
